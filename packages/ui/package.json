{
  "name": "@hashgraph/ui",
  "license": "MIT",
  "version": "0.0.0",
  "sideEffects": [
    "**/*.css"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist",
    "./styles.css": "./dist/index.css"
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup src",
    "build:fast": "tsup src",
    "lint": "TIMING=1 eslint src/ --cache .",
    "lint:fix": "eslint src/ --fix",
    "clean": "rimraf .turbo && rimraf dist && rimraf node_modules ",
    "check-types": "tsc --noEmit --pretty",
    "format:write": "prettier --write \"src/**/*.{ts,tsx}\" --cache",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\" --cache"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.2",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "eslint-config-custom": "workspace:*",
    "react": "^18.2.0",
    "tsconfig": "workspace:*",
    "tsup": "^6.0.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@floating-ui/react-dom": "^2.0.4",
    "@hashgraph/config": "workspace:*",
    "@hashgraph/utils": "workspace:*",
    "@radix-ui/primitive": "^1.0.1",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-popper": "^1.1.3",
    "@radix-ui/react-portal": "^1.0.4",
    "@radix-ui/react-primitive": "^1.0.3",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-use-controllable-state": "^1.0.1",
    "@storybook/react": "^7.5.3",
    "autoprefixer": "^10.4.14",
    "class-variance-authority": "^0.7.0",
    "postcss": "^8.4.27",
    "react-select": "^5.8.0",
    "tailwindcss": "^3.3.5",
    "tailwindcss-animate": "^1.0.7"
  },
  "publishConfig": {
    "access": "public"
  }
}
